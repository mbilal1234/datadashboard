    {
      "id": 802985070838426,
      "definition": {
        "title": "Access Errors",
        "type": "group",
        "layout_type": "ordered",
        "widgets": [
          {
            "id": 154574187474130,
            "definition": {
              "title": "Failed Logons",
              "title_size": "16",
              "title_align": "left",
              "show_legend": true,
              "legend_layout": "auto",
              "legend_columns": [
                "avg",
                "min",
                "max",
                "value",
                "sum"
              ],
              "type": "timeseries",
              "requests": [
                {
                  "on_right_yaxis": false,
                  "log_query": {
                    "index": "*",
                    "search": {
                      "query": "$env $appSite $product.value @logrecord.key:(ADC.Workbench.Remote.Session.Signon.Failed OR Cobalt.Website.Signon.Failed OR ADC.Workbench.Web.Signon.Failed OR com.trgr.ereader.server.CreateSession.ServerError OR \"OnePassAuthenticationService â€“ Authenticate\")"
                    },
                    "group_by": [
                      {
                        "facet": "@tr_product-name",
                        "sort": {
                          "order": "desc",
                          "aggregation": "count"
                        },
                        "limit": 10
                      }
                    ],
                    "compute": {
                      "interval": 300000,
                      "aggregation": "count"
                    }
                  },
                  "style": {
                    "palette": "warm",
                    "line_type": "solid",
                    "line_width": "normal"
                  },
                  "display_type": "bars"
                }
              ],
              "yaxis": {
                "min": "0"
              },
              "markers": []
            },
            "layout": {
              "x": 0,
              "y": 0,
              "width": 4,
              "height": 3
            }
          },
          {
            "id": 6715075790049464,
            "definition": {
              "title": "Disconnects",
              "title_size": "16",
              "title_align": "left",
              "show_legend": true,
              "legend_layout": "auto",
              "legend_columns": [
                "avg",
                "min",
                "max",
                "value",
                "sum"
              ],
              "type": "timeseries",
              "requests": [
                {
                  "formulas": [
                    {
                      "formula": "query1"
                    }
                  ],
                  "response_format": "timeseries",
                  "on_right_yaxis": false,
                  "queries": [
                    {
                      "search": {
                        "query": "$env $appSite @logrecord.key:(ADC.Workbench.UDSSession.OnWrongSite OR Cobalt.Website.UDSSession.OnWrongSite) $product.value"
                      },
                      "data_source": "logs",
                      "compute": {
                        "interval": 300000,
                        "aggregation": "count"
                      },
                      "name": "query1",
                      "indexes": [
                        "*"
                      ],
                      "group_by": [
                        {
                          "facet": "@tr_product-name",
                          "sort": {
                            "aggregation": "count",
                            "order": "desc"
                          },
                          "limit": 10
                        }
                      ]
                    }
                  ],
                  "style": {
                    "palette": "warm",
                    "line_type": "solid",
                    "line_width": "normal"
                  },
                  "display_type": "bars"
                }
              ],
              "yaxis": {
                "include_zero": true,
                "scale": "linear",
                "label": "",
                "min": "auto",
                "max": "auto"
              },
              "markers": []
            },
            "layout": {
              "x": 4,
              "y": 0,
              "width": 4,
              "height": 3
            }
          },
          {
            "id": 2094774712607174,
